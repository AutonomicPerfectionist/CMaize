#This is all the boilerplate CMake will take care of
@PACKAGE_INIT@

#This ensures that each dependency is locatable
foreach(_@CPP_project_name@_depend @_ci_DEPENDS@ )
    find_package(${_@CPP_project_name@_depend} REQUIRED)
endforeach()


include("${CMAKE_CURRENT_LIST_DIR}/@_ci_targets_file@.cmake")

#The following line conveys to find_package which components have been found
check_required_components("@CPP_project_name@")
